{
	"name": "cbt",
	"displayName": "Cognitive Behavioral Therapy Extension",
	"description": "Cognitive Behavioral Therapy Extension - tool for taking CBT notes",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:samples.quickInput"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "CBT",
			"properties": {
				"cbt.filePath": {
					"type": "string",
					"default": "",
					"description": "Set the exact location of the file"
				}
			}
		},
		"commands": [
			{
				"command": "samples.quickInput",
				"title": "Quick Input Samples"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.4.3"
	}
}
